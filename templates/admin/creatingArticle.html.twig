{% extends 'layout.html.twig' %}

{% block title %} Cr√©ation d'un articles - Admin{% endblock %}
{% block javascripts %}
    <script src="{{ asset('JS/form.js') }} " type="text/javascript"></script>
{% endblock %}
    {% block nav %}
        {% include "inc/nav_admin.html.twig" %}
    {% endblock %}
    {% block main %}
        <h1> CREATION D'UN ARTICLE</h1>
        {{ form_start(newArticleForm) }}
            <div class="new_cat" id="new_cat">
                {{ form_row(newArticleForm.category,{'attr':{"onchange":"newArticleAccess(this.value)"}} ) }}
            </div>
            <div class="new_access" id="new_access">
                {{ form_row(newArticleForm.access) }}
            </div>
            <div class="end_date" id="end_date">
                {{ form_row(newArticleForm.expDate) }}
            </div>

            {{ form_row(newArticleForm.ArtImg) }}
            {{ form_end(newArticleForm) }}


    {% endblock %}
