{% extends "layout.html.twig" %}
{% block javascripts %}
    <script src="{{ asset('JS/audit.js') }}" type="text/javascript"></script>
{% endblock %}
{% block main %}
   
<div class="container-fluid px-5 m-3">
    <div class="row  ">
        <div class="col-12">
         {% block flash %}
        {{ parent() }}
    {% endblock %}
 <p>
        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec feugiat sit amet sapien quis cursus.
        Quisque tellus tortor, finibus et vulputate at, porta ut velit. Integer laoreet tortor vitae sem hendrerit
        blandit.
    </p>
    {{ form_start(auditPartFourForm) }}
<div class="row">
    <div class="col-3 p-0">
    <table class="table audit3-table table-bordered bg-white">
        <thead>
        <tr>
            <th style="padding: 29px 6px !important;"></th>
        </tr>
        </thead>
        <tbody>
      {% for movableHeritageLabel in movableHeritageLabels %}
        <tr>
            <td style="padding: 16px 6px !important;"> {{ movableHeritageLabel.movableHeritageLabel}} </td>
        </tr>
      {% endfor %}
        </tbody>
    </table>
    </div>
    <div class="col-9 p-0">
    <table class="table audit3-table table-bordered bg-white">
        <thead>
        <tr>
            <th>Détenu par</th>
            <th>Ouvert en</th>
            <th>Montant détenu</th>
            <th>Organisme</th>
            <th>Taux d'intérêt</th>
            <th>Versement Mensuel/annuel</th>
            <th>Objectifs</th>
            <th>Bénéficiaires</th>
        </tr>
        </thead>
        <tbody>
        {% for movableHeritage in auditPartFourForm.movableHeritage  %}
        <tr>
            <td>{{ form_widget(movableHeritage.detainedBy) }}</td>
            <td>{{ form_widget(movableHeritage.openDate) }}</td>
            <td>{{ form_widget(movableHeritage.amount) }}</td>
            <td>{{ form_widget(movableHeritage.organization) }}</td>
            <td>{{ form_widget(movableHeritage.interestRate) }}</td>
            <td>{{ form_widget(movableHeritage.monthlyAnnualPayment) }}</td>
            <td>{{ form_widget(movableHeritage.goal) }}</td>
            <td>{{ form_widget(movableHeritage.beneficiary) }}</td>
        </tr>
         {% endfor %}
        </tbody>
    </table>
</div>
    </div>


    {{ form_end(auditPartFourForm) }}
        </div>
     </div>
 </div>

   
{% endblock %}